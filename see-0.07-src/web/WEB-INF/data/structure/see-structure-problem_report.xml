<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <type>matrix</type>
  <keyCanModify>no</keyCanModify>
  <defaultOrder>ascending</defaultOrder>
  <defaultColor>name:v:h-v:h:status:v:h-v:h:commit:v:h-v:h:FFDDDD:v:h-v:h:tested:v:h-v:h:DDFFFF:v:h-v:h:reviewed:v:h-v:h:FFFF99:v:h-v:h:denied:v:h-v:h:DDDDFF:v:h-v:h:fixed:v:h-v:h:FFDD99:v:h-v:h:defer:v:h-v:h:FFDDFF:v:h-v:h:closed:v:h-v:h:DDFFDD:v:h-v:h:others:v:h-v:h:EEEEEE</defaultColor>
  <matrixValues>commit:r:t-r:t:reviewed:r:t-r:t:fixed:r:t-r:t:tested:r:t-r:t:closed:r:t-r:t:defer:r:t-r:t:denied</matrixValues>
  <keys>configuration_code</keys>
  <notNullItems>title:-:status:-:description:-:reporter</notNullItems>
  <showItems>configuration_code:-:project:-:title:-:status:-:type:-:object:-:priority:-:assigned_to:-:last_changed_time</showItems>
  <operation>add:-:insert:-:copy:-:import:-:remove:-:modify:-:batch_modify:-:query:-:statistic:-:audit:-:select</operation>
  <items>
    <DataItem>
      <identify>configuration_code</identify>
      <type>string</type>
      <defaultValue>PR-</defaultValue>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>project</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>project</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>title</identify>
      <type>string</type>
    </DataItem>
    <DataItem>
      <identify>status</identify>
      <type>string</type>
      <defaultValue>commit</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>commit:-:reviewed:-:fixed:-:tested:-:closed:-:defer:-:denied</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>type</identify>
      <type>string</type>
      <defaultValue>bug</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>bug:-:advice:-:user_feedback:-:enhancement:-:not_implemented</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>object</identify>
      <type>string</type>
      <defaultValue>code</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>code:-:database:-:data:-:testing:-:design:-:requirement:-:document</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>severity</identify>
      <type>string</type>
      <defaultValue>medium</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>very_high:-:high:-:medium:-:low:-:very_low</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>priority</identify>
      <type>string</type>
      <defaultValue>medium</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>very_high:-:high:-:medium:-:low:-:very_low</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>description</identify>
      <type>string</type>
      <editWidth>0</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>effect_analysis</identify>
      <type>string</type>
      <editWidth>0</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>assigned_to</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>plan_effort</identify>
      <type>int</type>
      <unitage>person_day</unitage>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>plan_cost</identify>
      <type>int</type>
      <unitage>yuan</unitage>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>reporter</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>comment</identify>
      <type>string</type>
      <editWidth>0</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>attachment</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>file</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_code</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>code_register</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_doc</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>document_register</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>supervisor</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>actual_effort</identify>
      <type>string</type>
      <unitage>person_day</unitage>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>actual_cost</identify>
      <type>int</type>
      <unitage>yuan</unitage>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>testing_result</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>testing_result</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>conclusion</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>commit_person</identify>
      <type>string</type>
      <valueConstraintType>creatuser</valueConstraintType>
    </DataItem>
    <DataItem>
      <identify>commit_time</identify>
      <type>time</type>
      <valueConstraintType>creattime</valueConstraintType>
    </DataItem>
    <DataItem>
      <identify>last_changed_person</identify>
      <type>string</type>
      <valueConstraintType>currentuser</valueConstraintType>
    </DataItem>
    <DataItem>
      <identify>last_changed_time</identify>
      <type>time</type>
      <valueConstraintType>currenttime</valueConstraintType>
    </DataItem>
    <DataItem>
      <identify>who</identify>
      <type>string</type>
      <editWidth>10</editWidth>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>radio</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>when</identify>
      <type>time</type>
      <editWidth>30</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>explanation</identify>
      <type>string</type>
      <editWidth>25</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
  </items>
</DataSet>
