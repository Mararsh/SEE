<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <type>matrix</type>
  <keyCanModify>no</keyCanModify>
  <defaultOrder>ascending</defaultOrder>
  <defaultColor>name:v:h-v:h:status:v:h-v:h:commit:v:h-v:h:FFDDDD:v:h-v:h:authorized:v:h-v:h:FFDD99:v:h-v:h:review:v:h-v:h:FFFF99:v:h-v:h:implemented:v:h-v:h:DDFFDD:v:h-v:h:denied:v:h-v:h:DDFFFF:v:h-v:h:defer:v:h-v:h:DDDDFF:v:h-v:h:others:v:h-v:h:EEEEEE</defaultColor>
  <matrixValues>commit:r:t-r:t:review:r:t-r:t:authorized:r:t-r:t:denied:r:t-r:t:implemented:r:t-r:t:defer</matrixValues>
  <keys>configuration_code</keys>
  <notNullItems>title:-:type:-:status:-:description</notNullItems>
  <showItems>configuration_code:-:project:-:title:-:type:-:essentiality:-:level:-:status:-:last_changed_time</showItems>
  <operation>add:-:insert:-:copy:-:import:-:remove:-:modify:-:batch_modify:-:query:-:statistic:-:audit:-:select</operation>
  <items>
    <DataItem>
      <identify>configuration_code</identify>
      <type>string</type>
      <defaultValue>TR-</defaultValue>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>project</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>project</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>title</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>type</identify>
      <type>string</type>
      <defaultValue>function</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>multi</valueConstraintType>
      <valueConstraint>function:-:user_model:-:abnormity_dealing:-:boundary_checking:-:data_checking:-:interface:-:robust:-:performance:-:control_flow</valueConstraint>
      <editControllerType>check</editControllerType>
    </DataItem>
    <DataItem>
      <identify>essentiality</identify>
      <type>string</type>
      <defaultValue>medium</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>high:-:medium:-:low</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>level</identify>
      <type>string</type>
      <defaultValue>system</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>system:-:component:-:unit</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>status</identify>
      <type>string</type>
      <defaultValue>commit</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>commit:-:review:-:authorized:-:denied:-:implemented:-:defer</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>description</identify>
      <type>string</type>
      <editWidth>0</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>software_requirement</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>software_requirement</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_doc</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>document_register</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_code</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>code_register</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>commit_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creatuser</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>commit_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creattime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currentuser</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currenttime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>who</identify>
      <type>string</type>
      <editWidth>10</editWidth>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>when</identify>
      <type>time</type>
      <editWidth>30</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>explanation</identify>
      <type>string</type>
      <editWidth>25</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
  </items>
</DataSet>
