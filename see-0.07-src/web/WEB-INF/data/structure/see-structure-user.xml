<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <type>table</type>
  <keyCanModify>no</keyCanModify>
  <defaultOrder>ascending</defaultOrder>
  <keys>id</keys>
  <showItems>id:-:email:-:nickname:-:sex:-:birthday</showItems>
  <operation>add:-:insert:-:copy:-:import:-:remove:-:modify:-:batch_modify:-:query:-:statistic:-:audit:-:select</operation>
  <items>
    <DataItem>
      <identify>id</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>password</identify>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>password</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>email</identify>
      <type>string</type>
      <defaultValue>@</defaultValue>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>phone</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>nickname</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>sex</identify>
      <type>string</type>
      <defaultValue>female</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>female:-:male</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>birthday</identify>
      <type>date</type>
    </DataItem>
    <DataItem>
      <identify>interface_style</identify>
      <type>string</type>
      <defaultValue>style_default</defaultValue>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>interface_style</valueConstraintType>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>interface_theme</identify>
      <type>string</type>
      <defaultValue>theme_default</defaultValue>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>interface_theme</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>datalist_style</identify>
      <type>string</type>
      <defaultValue>table_style</defaultValue>
      <valueConstraintType>datalist_style</valueConstraintType>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>page_size</identify>
      <type>int</type>
      <defaultValue>10</defaultValue>
    </DataItem>
    <DataItem>
      <identify>default_project</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>project</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <type>string</type>
      <valueTranslated>yes</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>job_title</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>description</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>register_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creattime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currenttime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
  </items>
</DataSet>
