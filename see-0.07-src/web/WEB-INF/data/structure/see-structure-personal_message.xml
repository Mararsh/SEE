<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <type>table</type>
  <keyCanModify>no</keyCanModify>
  <defaultSort>sent_time</defaultSort>
  <defaultOrder>descending</defaultOrder>
  <defaultColor>name:v:h-v:h:have_read:v:h-v:h:no:v:h-v:h:DDFFDD</defaultColor>
  <keys>id</keys>
  <notNullItems>title:-:sender:-:receiver:-:have_read:-:content</notNullItems>
  <showItems>title:-:have_read:-:sender:-:receiver:-:sent_time</showItems>
  <operation>add:-:remove:-:query</operation>
  <items>
    <DataItem>
      <identify>id</identify>
      <type>auto</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>have_read</identify>
      <type>string</type>
      <defaultValue>no</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>readonly</valueConstraintType>
    </DataItem>
    <DataItem>
      <identify>title</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>sender</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creatuser</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>sent_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creattime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>receiver</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>check</editControllerType>
    </DataItem>
    <DataItem>
      <identify>content</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>link</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>link</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>attachment</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>file</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
  </items>
</DataSet>
