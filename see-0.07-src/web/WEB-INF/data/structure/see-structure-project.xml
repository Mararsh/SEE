<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <type>matrix</type>
  <keyCanModify>no</keyCanModify>
  <defaultOrder>ascending</defaultOrder>
  <defaultColor>name:v:h-v:h:status:v:h-v:h:applying:v:h-v:h:FFDDDD:v:h-v:h:approved:v:h-v:h:FFDD99:v:h-v:h:requirement:v:h-v:h:FFFF99:v:h-v:h:validated:v:h-v:h:DDFFDD:v:h-v:h:design:v:h-v:h:DDFFFF:v:h-v:h:implementing:v:h-v:h:DDDDFF:v:h-v:h:testing:v:h-v:h:FFDDFF:v:h-v:h:others:v:h-v:h:EEEEEE</defaultColor>
  <matrixValues>plan_whole:r:t-r:t:actual_whole:r:t-r:t:requirement_analysis:r:t-r:t:design:r:t-r:t:coding:r:t-r:t:independant_testing:r:t-r:t:document_edit:r:t-r:t:project_management:r:t-r:t:quality_management:r:t-r:t:configuration_management:r:t-r:t:training:r:t-r:t:maintenance:r:t-r:t:technical_supporting:r:t-r:t:others</matrixValues>
  <keys>configuration_code</keys>
  <notNullItems>name:-:project_manager</notNullItems>
  <showItems>configuration_code:-:name:-:project_manager:-:status</showItems>
  <operation>add:-:insert:-:copy:-:import:-:remove:-:modify:-:batch_modify:-:query:-:statistic:-:audit:-:select</operation>
  <items>
    <DataItem>
      <identify>configuration_code</identify>
      <type>string</type>
      <defaultValue>P-</defaultValue>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>name</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>project_manager</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>status</identify>
      <type>string</type>
      <defaultValue>applying</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>applying:-:approved:-:requirement:-:design:-:implementing:-:testing:-:validated:-:maintenance:-:closed:-:denied</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>project_role</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>project_role</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>member</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>authorizer</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>authorize_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>description</identify>
      <type>string</type>
      <size>0</size>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>start_date</identify>
      <type>date</type>
      <editWidth>12</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>end_date</identify>
      <type>date</type>
      <editWidth>12</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>staff_number</identify>
      <type>int</type>
      <editWidth>8</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>size</identify>
      <type>int</type>
      <editWidth>8</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>effort</identify>
      <type>int</type>
      <unitage>person_month</unitage>
      <editWidth>8</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>cost</identify>
      <type>int</type>
      <unitage>yuan</unitage>
      <editWidth>8</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
      <ismatrix>yes</ismatrix>
    </DataItem>
    <DataItem>
      <identify>relative_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>commit_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creatuser</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>commit_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creattime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currentuser</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currenttime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
  </items>
</DataSet>
