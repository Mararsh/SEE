<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <type>table</type>
  <keyCanModify>no</keyCanModify>
  <defaultOrder>ascending</defaultOrder>
  <defaultColor>name:v:h-v:h:conclusion:v:h-v:h:not_pass:v:h-v:h:FFDDDD:v:h-v:h:part_pass:v:h-v:h:FFDD99:v:h-v:h:pass:v:h-v:h:DDFFDD:v:h-v:h:others:v:h-v:h:EEEEEE</defaultColor>
  <keys>configuration_code</keys>
  <notNullItems>conclusion:-:actual_result:-:tester:-:testing_time</notNullItems>
  <showItems>configuration_code:-:project:-:testing_procedure:-:conclusion:-:last_changed_time</showItems>
  <operation>add:-:insert:-:copy:-:import:-:remove:-:modify:-:batch_modify:-:query:-:statistic:-:audit:-:select</operation>
  <items>
    <DataItem>
      <identify>configuration_code</identify>
      <type>string</type>
      <defaultValue>TS-</defaultValue>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>project</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>project</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>testing_procedure</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>testing_procedure</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>conclusion</identify>
      <type>string</type>
      <defaultValue>pass</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>pass:-:part_pass:-:not_pass</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>actual_result</identify>
      <type>string</type>
      <editWidth>0</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>comment</identify>
      <type>string</type>
      <editWidth>0</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>problem_report </identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>problem_report </valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>tester</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>testing_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_doc</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>document_register</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_code</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>code_register</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>commit_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creatuser</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>commit_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>creattime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currentuser</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currenttime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
  </items>
</DataSet>
