<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <type>table</type>
  <keyCanModify>yes</keyCanModify>
  <defaultOrder>ascending</defaultOrder>
  <defaultColor>name:v:h-v:h:conclusion:v:h-v:h:denied:v:h-v:h:FFDDDD:v:h-v:h:modify:v:h-v:h:FFDD99:v:h-v:h:pass:v:h-v:h:DDFFDD:v:h-v:h:defer:v:h-v:h:DDDDFF:v:h-v:h:others:v:h-v:h:EEEEEE</defaultColor>
  <keys>configuration_code</keys>
  <notNullItems>code:-:time:-:conclusion</notNullItems>
  <showItems>configuration_code:-:code:-:time:-:conclusion:-:authorizer:-:last_changed_time</showItems>
  <operation>add:-:insert:-:copy:-:import:-:remove:-:modify:-:batch_modify:-:query:-:statistic:-:audit:-:select</operation>
  <items>
    <DataItem>
      <identify>configuration_code</identify>
      <type>string</type>
      <defaultValue>CR-</defaultValue>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>code</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>code_register</valueConstraint>
      <editControllerType>check</editControllerType>
    </DataItem>
    <DataItem>
      <identify>time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>conclusion</identify>
      <type>string</type>
      <defaultValue>pass</defaultValue>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>pass:-:modify:-:denied:-:defer</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>review_persons</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>authorizer</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_solo</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>description</identify>
      <type>string</type>
      <editWidth>0</editWidth>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_doc</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>document_register</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_code</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>code_register</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>relative_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>user</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_person</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currentuser</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>last_changed_time</identify>
      <type>time</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>currenttime</valueConstraintType>
      <editControllerType>text</editControllerType>
    </DataItem>
  </items>
</DataSet>
