<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <type>table</type>
  <keyCanModify>yes</keyCanModify>
  <defaultOrder>ascending</defaultOrder>
  <keys>title</keys>
  <notNullItems>data:-:system_role:-:operation</notNullItems>
  <showItems>title:-:data:-:system_role:-:operation:-:condition</showItems>
  <operation>add:-:insert:-:copy:-:import:-:remove:-:modify:-:batch_modify:-:query:-:statistic:-:audit:-:select</operation>
  <items>
    <DataItem>
      <identify>title</identify>
      <type>string</type>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>data</identify>
      <type>string</type>
      <valueConstraintType>multi</valueConstraintType>
      <valueConstraint>*:-:problem_report:-:working_log:-:user:-:role:-:data_user_acl:-:data_role_acl:-:data_audit</valueConstraint>
      <editControllerType>check</editControllerType>
    </DataItem>
    <DataItem>
      <identify>system_role</identify>
      <type>string</type>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>ref_multi</valueConstraintType>
      <valueConstraint>system_role</valueConstraint>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>operation</identify>
      <type>string</type>
      <valueTranslated>yes</valueTranslated>
      <valueConstraintType>multi</valueConstraintType>
      <editControllerType>check</editControllerType>
    </DataItem>
    <DataItem>
      <identify>condition</identify>
      <type>string</type>
    </DataItem>
    <DataItem>
      <identify>description</identify>
      <type>string</type>
      <size>0</size>
      <valueTranslated>no</valueTranslated>
      <editControllerType>textarea</editControllerType>
    </DataItem>
  </items>
</DataSet>
