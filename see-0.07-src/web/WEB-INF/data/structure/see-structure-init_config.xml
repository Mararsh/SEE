<?xml version="1.0" encoding="UTF-8"?>
<DataSet>
  <keyCanModify>no</keyCanModify>
  <defaultOrder>ascending</defaultOrder>
  <keys>id</keys>
  <invalidItems>data_source:-:DBDriver:-:DBUser:-:DBPassword</invalidItems>
  <operation>modify:-:query:-:audit</operation>
  <items>
    <DataItem>
      <identify>id</identify>
      <type>string</type>
      <defaultValue>init</defaultValue>
    </DataItem>
    <DataItem>
      <identify>default_user</identify>
      <type>string</type>
      <defaultValue>guest</defaultValue>
    </DataItem>
    <DataItem>
      <identify>default_interface_style</identify>
      <type>string</type>
      <defaultValue>style_default</defaultValue>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>style_default</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>default_interface_theme</identify>
      <defaultValue>theme_default</defaultValue>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>theme_default</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>default_page_size</identify>
      <type>int</type>
      <defaultValue>8</defaultValue>
    </DataItem>
    <DataItem>
      <identify>data_audit_types</identify>
      <type>string</type>
      <valueConstraintType>data_audit_types</valueConstraintType>
      <editControllerType>check</editControllerType>
    </DataItem>
    <DataItem>
      <identify>default_upload_size</identify>
      <type>int</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>default_upload_total</identify>
      <type>int</type>
      <valueTranslated>no</valueTranslated>
      <editControllerType>text</editControllerType>
    </DataItem>
    <DataItem>
      <identify>data_source</identify>
      <type>string</type>
      <defaultValue>xml</defaultValue>
      <valueTranslated>no</valueTranslated>
      <valueConstraintType>solo</valueConstraintType>
      <valueConstraint>xml:-:database</valueConstraint>
      <editControllerType>radio</editControllerType>
    </DataItem>
    <DataItem>
      <identify>DBDriver</identify>
      <type>string</type>
      <defaultValue>t.mm.mysql.Driver</defaultValue>
    </DataItem>
    <DataItem>
      <identify>DBUser</identify>
      <type>string</type>
      <defaultValue>mara</defaultValue>
    </DataItem>
    <DataItem>
      <identify>DBPassword</identify>
      <type>string</type>
      <defaultValue>mara</defaultValue>
    </DataItem>
  </items>
</DataSet>
